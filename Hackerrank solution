#include<stdio.h>

int main()
{
   int a[3],b[3],i=0,j=0,count=0,count1=0;

    for(i=0;i<3;i++)
      {  scanf("%d",&a[i]); }

for(i=0;i<3;i++)
   {  scanf("%d",&b[i]);  }

for(i=0;i<3;i++)  
    {  if(a[i]>b[i]&&a[i]!=b[i])
           count++;
       else
           if(b[i]>a[i]&&a[i]!=b[i])
              count1++; 
              }
printf("%d %d",count,count1);

return 0;
}
